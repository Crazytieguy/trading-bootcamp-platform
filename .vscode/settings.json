{
  "protoc": {
    "options": ["--proto_path=schemas"]
  },
  "files.associations": {
    "*.css": "tailwindcss",
    "*.postcss": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "prettier.documentSelectors": ["**/*.svelte"],
  "svelte.enable-ts-plugin": true,
  "eslint.validate": ["svelte"]
}
