{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "generate-schema && vite",
    "generate-schema": "pbjs -t static-module -w commonjs -o src/schema/index.js ../schema/server-message.proto && pbts -o src/schema/index.d.ts src/schema/index.js",
    "build": "generate-schema && tsc && vite build",
    "preview": "generate-schema && vite preview"
  },
  "dependencies": {
    "protobufjs": "^7.3.2",
    "solid-js": "^1.8.15",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "protobufjs-cli": "^1.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-solid": "^2.10.2"
  }
}
